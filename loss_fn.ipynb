    "import import_ipynb\n",
    "import torch\n",
    "import torch.nn as nn\n",
    "\n",
    "\n",
    "def Selective_Regularization(y_hat, target, wp, bp):\n",
    "    loss_fn = nn.BCELoss()\n",
    "    return loss_fn(y_hat, target) + (torch.norm(wp,p='fro')+torch.norm(bp))"
   ]
